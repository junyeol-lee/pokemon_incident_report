<h2>Pokemon Next</h2>
<h2><%= @nest.historicalName%></h2>
<h4>streetName: <%= @nest.streetName%></h4>
<h4>streetNumber: <%= @nest.streetNumber%></h4>
<h4>grade: <%= @nest.grade%></h4>
<h4>constructionDate: <%= @nest.constructionDate%></h4>
<h4>Host pokemon: <%= @nest.pokemon.name%></h4>

  <div id="map"></div>
  <script>
    var map;
    var marker;
    var myLatLng = {lat: parseFloat("<%=@nest.latitude%>"), lng: parseFloat("<%=@nest.longitude%>")}
    function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
        center: {lat: parseFloat("<%=@nest.latitude%>"), lng: parseFloat("<%=@nest.longitude%>")},
        zoom: 16
      });
      marker = new google.maps.Marker({
        position: myLatLng,
        map: map,
        title: 'Incident here!'
      });
    }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?AIzaSyDxWX88W5DMaeTsp7T44PmiB4Su3z336fk&callback=initMap"
  async defer></script>
